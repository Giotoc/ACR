<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://asyncode.com/View"
  xmlns:fs="http://asyncode.com/filesystem"
  xmlns:h="http://asyncode.com/headers">
  <post>
    <param name="viewName"/>
    <param name="content"/>
  </post>
  <define name="fileUpdated" command="fs:update" fs:path="/views/{$viewName}">{$content}</define>
    <define name="aaa" command="h:redirect" h:location="/IDE:editView/{$viewName}/"/>
</view>
