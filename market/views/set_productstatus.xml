<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://asyncode.com/View" xmlns:db="http://asyncode.com/DataBase" xmlns:fs="http://asyncode.com/filesystem">
	<post>
		<param name="status"/> <!--hidden, test, beta, avaible -->
	</post>
	<define name="productstatus" command="db:query" condition="rs::status is not ''">
		update market.product set
	    status='{$status}'
		where id=1
	</define>
</view>