<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<blockt type="container w974 inner body">
		<widget name="newsList" type="list" dataSource="news" titleID="news" schema="news">
			<template name="item twothirds">
				<h2>
					<img src="" alt="">
						<pars for="src">
							<par>http://e.fstatic.eu/flags/</par>
							<par from="lang"/>
							<par>.png</par>
						</pars>
						<pars for="alt">
							<par from="lang"/>
						</pars>
					</img>
					<node name="title"/>
				</h2>
				<div class="lang">
				</div>
				<div class="content"><node name="content"/></div>
				<div class="date">
					Posted <node name="create_date"/>
				</div>
			</template>
		</widget>
		<widget name="paginate" type="paginate" parsSource="newsPars"/>
	</blockt>
</layout>
